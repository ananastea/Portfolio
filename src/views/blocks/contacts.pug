section.contacts
  .contacts__map-container
    iframe.contacts__map(src="www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2533.6366876100446!2d36.556618415580054!3d50.57811838603878!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x41266a800b288883%3A0x64ad756e80fe6ef0!2z0YPQuy4g0JvRjNCy0LAg0KLQvtC70YHRgtC-0LPQviwg0JHQtdC70LPQvtGA0L7QtCwg0JHQtdC70LPQvtGA0L7QtNGB0LrQsNGPINC-0LHQuy4!5e0!3m2!1sru!2sru!4v1547371310529" width="100%" height="100%" frameborder="0" style="border:0" allowfullscreen)
  .contacts__block
    h2.contacts__title.title.title--small Контакты
    ul.contacts__list
      -
        var contacts = [
        {name:'skype', text:'+79155618684', href: 'skype:+79155618684'},
        {name:'envelope', text:'barsukova.nastya@mail.ru', href: 'mailto:barsukova.nastya@mail.ru'},
        {name:'phone', text:'+79155618684', href: 'tel:+79155618684'},
        {name:'map_marker', text:'г.Белгород, ул. Толстого', href: ''}
        ]
      each contact in contacts
        li.contacts__item
          a.contacts__link(href=`${contact.href}`)
            +icon(`${contact.name}`, 'contacts__img')
            span.contacts__text #{contact.text}
